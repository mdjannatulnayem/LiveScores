<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@HomeStyle.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LiveScores.mainUI.HomeController">
   <children>
      <Label layoutX="190.0" layoutY="21.0" text="LiveScores">
         <font>
            <Font name="Cambria" size="48.0" />
         </font>
      </Label>
      <Button layoutX="232.0" layoutY="296.0" mnemonicParsing="false" onAction="#goToMatch" prefHeight="39.0" prefWidth="137.0" text="Go to match">
         <font>
            <Font name="Cambria" size="20.0" />
         </font>
      </Button>
      <Button layoutX="231.0" layoutY="341.0" mnemonicParsing="false" onAction="#updateMatch" text="Update match">
         <font>
            <Font name="Cambria" size="19.0" />
