         <font>
            <Font name="Cambria" size="20.0" />
         </font>
      </Button>
      <Button layoutX="233.0" layoutY="172.0" mnemonicParsing="false" onAction="#____addplayer" prefHeight="39.0" prefWidth="137.0" text="Add player">
         <font>
            <Font name="Cambria" size="20.0" />
         </font>
      </Button>
